<% if @links.size.positive? %>
  <div class="content">
    <div class="container-xl">
      <div class="page-header">
        <div class="row align-items-center">
          <div class="col-auto">
            <h2 class="page-title">
              Dashboard
            </h2>
          </div>
        </div>
      </div>

      <div class="card">
        <ul class="list-group card-list-group">
          <% @links.each do |link| %>
            <%= render "links/link", link: link, show_link: true %>
          <% end %>
        </ul>
      </div>
    </div>
  </div>
<% else %>
  <%= render "blank" %>
<% end %>
